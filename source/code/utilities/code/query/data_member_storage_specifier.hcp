◀public: static▶ Storage_Specifier ☀Get(model::DataMember const& mem)❰

    auto str = First_Element_Or_Default(mem.class_specifiers).value;
    
    Storage_Specifier s = Storage_Specifier::NONE;
    
    Token_Attribute_Settings symbols;
    if (Contains_Substring(str,symbols.data_member.static_storage)){
        s = Storage_Specifier::STATIC;
    }
    else if (Contains_Substring(str,symbols.data_member.extern_storage)){
        s = Storage_Specifier::EXTERN;
    }
    return s;
