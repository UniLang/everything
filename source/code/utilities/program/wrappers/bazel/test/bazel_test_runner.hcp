◀public: static▶ bool ☀Test(std::string const& bazel_wrapper, std::string const& target)❰
    
    
    //build command
    auto command = Build_Command(bazel_wrapper, target);
    
    //run command
    auto spawn_results = Timed_Process_Spawner::Execute_And_Get_Back_Results(command);
    
    return spawn_results.results.return_code == 0;
    
❱
◀private: static▶ std::string ☀Build_Command(std::string const& bazel_wrapper, std::string const& target)❰

    std::string command = bazel_wrapper;
    command += " test ";
    
    command += "--noremote_accept_cached ";
    command += "--nocache_test_results ";
    command += "--config=remote ";
    command += target;
    return command;
