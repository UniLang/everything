        x += Get_Type_Specializations(it);
        x += " ";
        x += Data_Member_To_Identifier::Derive_Name(it,settings.module,false,true);
        x += Default_Initialize_Getter::Possibly_Get_Default_Value(it,Source_Language::JAVA);
        x += ";\n\n";

    }
    
    return x;
