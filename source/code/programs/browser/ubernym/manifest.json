{
"manifest_version": 2,
    "name": "Ubernym",
    "description": "Define company acronyms in your browser",
    "version": "1.0",
    "web_accessible_resources": [
        "acronym.json"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "css": ["tooltip.css"],
            "run_at": "document_end"
        }
    ]
}